    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug / Run : run a python file",
            "type": "python",
            "request": "launch",
            "program": "${fileBasenameNoExtension}.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Debug / Run : run webipy test file.",
            "type": "python",
            "request": "launch",
            "program": "test.py",
            "console": "integratedTerminal"
        },
        {
            "type": "pwa-chrome",
            "name": "Launch localhost on WebIpy docs.",
            "request": "launch",
            "url": "http://127.0.0.1:5500/docs/index.html"
        }
    ]
}